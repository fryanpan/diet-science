  "scripts": {
    "test": "nyc mocha -r ts-node/register 'src/**/*.test.ts'",
    "watch": "mocha --watch -r ts-node/register --reporter=nyan 'src/**/*.test.ts'",
    "start": "node --inspect=5858 -r ts-node/register ./src/server.ts",
    "start_dist": "node dist/server.js",
    "sequelize-cli": "npm run build && node_modules/.bin/sequelize",
    "deploy:dev": "npm run build && serverless deploy --stage dev",
    "deploy:prod": "npm run build && serverless deploy --stage prod",
    "update_schema": "node_modules/apollo/bin/run service:download --endpoint=http://localhost:4000/dev/graphql  '../ios/Diet Science Watchkit Extension/schema.json'",
