    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "graphql": "^15.0.0",
    "graphql-tools": "^5.0.0",
    "http": "0.0.1-security",
    "morgan": "^1.10.0",
    "ncp": "^2.0.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/graphql": "^14.5.0",
    "@types/graphql-depth-limit": "^1.1.2",
    "@types/mocha": "^7.0.2",
    "@types/morgan": "^1.9.0",
    "@types/node": "^14.0.1",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "eslint": "^7.0.0",
    "eslint-plugin-import": "^2.20.2",
